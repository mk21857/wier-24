						jQuery(this).find('.show-day').find('.spored-item').eq(now_id + 1).children('.content').before('<div class="playing-next"><span>sledi</span> </div>');
					}
					jQuery(this).find('.show-day').find('.spored-item').eq(next_id).removeClass('Next');
